<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginEnd="@dimen/dp_12">
    <FrameLayout android:layout_width="@dimen/dp_54" android:layout_height="@dimen/dp_54" android:layout_marginTop="@dimen/dp_10" android:layout_marginEnd="@dimen/dp_10">
        <androidx.cardview.widget.CardView android:id="@+id/item_photo_card" android:layout_width="match_parent" android:layout_height="match_parent" app:cardBackgroundColor="@color/transparent" app:cardCornerRadius="@dimen/dp_8" app:cardElevation="0dp">
